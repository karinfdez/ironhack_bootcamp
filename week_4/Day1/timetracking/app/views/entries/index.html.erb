	   	<a href="/projects/<%=@proj.id %>/entries/<%=entr.id %>/edit">Edit</a>
	   	<a href="/projects/<%=@proj.id %>/entries/<%=entr.id %>">Delete</a>
	   </li>
	 <% end%>
</ul>
<a href="/projects/<%= @proj.id%>/entries/new">add new entry</a>
<h1>List of users:</h1>

<%if @users.count!=0%>
	
	<table>
  <tr>
    <th>Users</th>
     <th>Created on:</th>
   
  </tr>
 
	<% @users.each do |user| %>
  	<tr>
    	<td><%= user.name%></td>
    	<td><%= user.email%></td>
    	<td></td>
    	<td></td>
    	<!-- <td><%= post.content %></td> -->
    	<td><%= link_to 'Show', user %></td>
    	<td><%= link_to 'Edit', edit_user_path(user) %></td>
    	<td><%= link_to 'Destroy',user, :confirm => 'Are you sure?',
                                     	  :method => :delete %></td>
 	</tr>
	<% end %>
	</table>
	<br />
	<%# end %>	
<% else %>
	<p>THERE IS NO USERS TO SHOW.</p>
<% end %>

 
<%= link_to 'New User', new_user_path %>

<h1>Timetracking tool</h1>

<h2>Project entries</h2>

<% if flash[:notice].present? %>
	<div class="notice">
		<%= flash[:notice] %>
	</div>
<% end %>

<h3>
	<%= @proj.name%>
</h3>

<% if flash[:notice].present? %>
  <div class="notice">
    <%= flash[:notice] %>
  </div>
<% end %>

<ul>
	<% @entr.each do |entr|%>
 		<li>
 		<%= entr.hours %> hours 	
